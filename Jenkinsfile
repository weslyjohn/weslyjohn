pipeline {
    agent any

    stages {
        stage('dev') {
            steps {
                echo ' application is devoloped'
            }
        }
         stage('testing') {
            steps {
                echo 'Automation testing'
            }
        }
         stage('UAT') {
            steps {
                echo 'Waiting for user acceptancy'
            }
        }
         stage('release') {
            steps {
                echo 'application is ready to realse'
            }
        }
    }
}
